{
  "name": "js-pollock",
  "author": "Matt Jarrett",
  "license": "MIT",
  "description": "Single Page App for computer generated art inspired by Jackson Pollock",
  "version": "0.7.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:cujarrett/js-pollock.git"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "scripts": {
    "commit": "git-cz",
    "start": "react-scripts start",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "ci": "npm run lint",
    "build": "react-scripts build",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "keywords": [
    "Jackson Pollock",
    "Computer Generated Art"
  ],
  "dependencies": {
    "@material-ui/core": "~4.12.3",
    "@material-ui/icons": "~4.11.2",
    "clamp": "~1.0.1",
    "color-luminance": "~2.1.0",
    "file-saver": "~2.0.5",
    "font-awesome": "~4.7.0",
    "gl-vec2": "~1.3.0",
    "invert-color": "~2.0.0",
    "lerp": "~1.0.3",
    "new-array": "~1.0.0",
    "prop-types": "~15.7.2",
    "raf-loop": "~1.1.3",
    "react": "~17.0.2",
    "react-animated-transitions": "~0.0.6",
    "react-dom": "~17.0.2",
    "react-scripts": "~4.0.3",
    "react-transition-group": "~4.4.2",
    "seed-random": "~2.2.0",
    "simplex-noise": "~3.0.0",
    "smoothstep": "~1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "~13.2.1",
    "@commitlint/config-conventional": "~13.2.0",
    "@semantic-release/changelog": "~6.0.0",
    "@semantic-release/commit-analyzer": "~9.0.1",
    "@semantic-release/exec": "~6.0.1",
    "@semantic-release/git": "~10.0.0",
    "@semantic-release/github": "~8.0.1",
    "@semantic-release/npm": "~8.0.0",
    "@semantic-release/release-notes-generator": "~10.0.2",
    "commitizen": "~4.2.4",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~7.32.0",
    "husky": "~7.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
